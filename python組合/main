from Body import Body
from Teacher import Teacher
teacher1 = Teacher('Candy', Body(1.7, 80), 88000)
teacher2 = Teacher('Linda', Body(1.6, 90), 61000)
list=[]
for i in range(3):
    list.append(input())
teacher1.setName(list[0])
teacher2.setBody(Body(float(list[1]),int(list[2])))
teacher1.toString(teacher1.getName(),teacher1.getBody(),teacher1.getSalary())
teacher2.toString(teacher2.getName(),teacher2.getBody(),teacher2.getSalary())
