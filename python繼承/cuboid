from Shape import Shape
class Cuboid(Shape):
    def __str__(self):
        return "Cuboid"
    def __init__(self,length,width,height):
        self.__length=length
        self.__width=width
        self.__height=height
    def setLength(self,length):
        self.__length=length
    def setWidth(self,width):
        self.__width=width
    def setHeight(self,height):
        self.__height=height
    def getLength(self):
        return self.__length
    def getWidth(self):
        return self.__width
    def getHeight(self):
        return self.__height
    def getVolume(self):
        return self.__length*self.__width*self.__height*1.0
