from Shape import Shape
class Sphere(Shape):
    def __str__(self):
        return "Sphere"
    def __init__(self,radius):
        self.__radius=radius
    def setRadius(self,radius):
        self.__radius=radius
    def getRadius(self):
        return self.__radius
    def getVolume(self):
        return 4*3.14159*(self.__radius**3)/3
