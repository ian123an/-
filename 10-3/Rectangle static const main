#include<iostream>
#include "Rectangle.h"
using namespace std;

int main()
{
	double x, y;
	Rectangle array[3];
	for (int i = 0;i < 3;i++)
	{
		cin >> x >> y;
		array[i].setWidth(x);
		array[i].setHeight(y);
	}
	for (int i = 0;i < 3;i++)
	{
		cout << array[i].getWidth() << " " << array[i].getHeight() << " " << array[i].getArea() << endl;
	}
	cout << array[2].getNumOfRect() << " " << "Rectangles" << endl;
}
